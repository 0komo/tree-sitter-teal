    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter grammar for Teal",
    "authors": [
      {
        "name": "Komo"
      }
    ],
    "links": {
      "repository": "https://github.com/0komo/tree-sitter-teal"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}